  pages = {181--184},
  affiliation = {Dipartimento Scienze Informazione, Universit{\`a} degli Studi di Milano, Milan, Italy; Dipart. Elettronica e Informazione, Politecnico di Milano, Milan, Italy},
  author_keywords = {Distance learning; Distributed collaboration; Mobile applications; Reasoning and recommendations; User profiling; Web services},
  document_type = {Conference Paper},
