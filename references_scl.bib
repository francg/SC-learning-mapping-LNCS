  note = {cited By 0},
  art_number = {6039464},
  affiliation = {BBRM Group of Consultants, CA 95757, United States; University of Texas at Arlington, Arlington, TX, United States},
  author_keywords = {Biomass; Integrated sustainable development; Microgrid; Renewable energy},
  document_type = {Conference Paper},
